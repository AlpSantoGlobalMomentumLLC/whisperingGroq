{
	"name": "with-svelte",
	"displayName": "With svelte",
	"version": "0.0.0",
	"description": "A basic Plasmo extension.",
	"author": "lgvic",
	"packageManager": "pnpm@7.5.0",
	"scripts": {
		"dev": "plasmo dev",
		"build": "plasmo build"
	},
	"dependencies": {
		"@plasmohq/storage": "^1.6.0",
		"plasmo": "0.57.2",
		"svelte": "3.52.0",
		"svelte-french-toast": "1.0.4-beta.0",
		"svelte-preprocess": "4.10.7"
	},
	"devDependencies": {
		"@plasmohq/prettier-plugin-sort-imports": "3.5.4",
		"@types/chrome": "0.0.198",
		"@types/node": "18.11.2",
		"autoprefixer": "^10.4.14",
		"postcss": "^8.4.23",
		"prettier": "2.7.1",
		"prettier-plugin-svelte": "^2.10.0",
		"tailwindcss": "^3.3.2",
		"typescript": "4.8.4"
	},
	"manifest": {
		"host_permissions": [
			"https://*/*"
		],
		"permissions": [
			"clipboardWrite",
			"commands",
			"tabs",
			"action"
		],
		"commands": {
			"toggle-recording": {
				"suggested_key": {
					"default": "Ctrl+Shift+I",
					"mac": "Command+Shift+I"
				},
				"description": "Toggle recording"
			}
		}
	}
}
